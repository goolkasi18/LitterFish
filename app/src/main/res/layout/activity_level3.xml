<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.wills.bouncingballactivity.Level1"
    android:background="@drawable/level3"
    android:id="@+id/level3back">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/badies3"
        android:onClick="dismissPopup"
        android:visibility="visible">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:id="@+id/imageView19"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="90dp"
            android:layout_marginBottom="55dp" />

        <ImageView
            android:layout_width="475dp"
            android:layout_height="60dp"
            android:id="@+id/imageView20"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="180dp" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="250dp"
            android:id="@+id/imageView21"
            android:layout_below="@+id/imageView20"
            android:layout_alignEnd="@+id/imageView20" />

        <ImageView
            android:layout_width="68dp"
            android:layout_height="240dp"
            android:id="@+id/imageView22"
            android:layout_below="@+id/imageView20"
            android:layout_alignStart="@+id/imageView20" />

        <ImageView
            android:layout_width="35dp"
            android:layout_height="45dp"
            android:id="@+id/imageView23"
            android:layout_marginTop="74dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView21"
            android:layout_marginRight="130dp" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageView24"
            android:layout_alignTop="@+id/imageView23"
            android:layout_toStartOf="@+id/imageView23"
            android:layout_marginRight="35dp"
            android:layout_marginTop="-15dp" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:id="@+id/imageView25"
            android:layout_alignTop="@+id/imageView24"
            android:layout_toStartOf="@+id/imageView24"
            android:layout_marginRight="55dp"
            android:layout_marginTop="10dp" />

        <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/imageView26"
            android:layout_below="@+id/imageView25"
            android:layout_alignStart="@+id/imageView25"
            android:layout_marginTop="13dp"
            android:layout_marginLeft="-17dp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/goodies3"
        android:visibility="visible">

        <ImageView
            android:layout_width="75dp"
            android:layout_height="35dp"
            android:id="@+id/imageView17"
            android:layout_marginRight="298dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="86dp" />

        <ImageView
            android:layout_width="75dp"
            android:layout_height="35dp"
            android:id="@+id/imageView18"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="97dp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/foods3"
        android:visibility="visible">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView31"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:background="@drawable/food"
            android:layout_marginRight="293dp"
            android:layout_marginTop="87dp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView30"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="@drawable/food"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="98dp" />

    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:visibility="visible">
        <com.example.wills.bouncingballactivity.CustomDrawableView3
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:id="@+id/LoseScreen"
        android:visibility="gone" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView34"
            android:background="@drawable/tryagain" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button8"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/yes"
            android:onClick="rePlay"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button9"
            android:layout_below="@+id/button8"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="275dp"
            android:background="@drawable/no"
            android:onClick="toLevelSelect"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/winscreen"
        android:onClick="toLevelSelect"
        android:visibility="gone"
        android:id="@+id/WinScreen" />

</RelativeLayout>

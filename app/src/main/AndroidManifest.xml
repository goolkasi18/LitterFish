<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wills.bouncingballactivity" >

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".BouncingBallActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LevelSelect"
            android:label="@string/title_activity_level_select"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Level1"
            android:label="@string/title_activity_level1"
            android:screenOrientation="landscape" >
        </activity>
        <activity
        android:name=".Level2"
        android:label="@string/title_activity_level2" >
    </activity>
        <activity
            android:name=".Level3"
            android:label="@string/title_activity_level2" >
        </activity>
    </application>

</manifest>
